@typeparam TItem

<CascadingValue Name="TreeEditOptions" Value="Options">
    <CascadingValue Name="Parent" Value="this">
        <ul class="treeview list-unstyled">
            @foreach (var item in Items)
            {
                @Options.ChildTemplate(item)
            }
        </ul>
    </CascadingValue>
</CascadingValue>


