@page "{customerId:guid}"
@model Resurgam.Admin.Web.Pages.CustomerModel
@{
    ViewData["Title"] = "Customer";
}

@section HeaderNav {
    <partial name="Shared/HeaderNavPartial" model="Model.HeaderNav" />
}

<table>
    <thead>
        <tr>
            <td>Project Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var project in Model.Projects)
        {
            <tr>
                <td><a asp-page="\Project" asp-route-projectId="@project.ProjectId">@project.ProjectName</a> </td>
            </tr>
        }
    </tbody>
</table>